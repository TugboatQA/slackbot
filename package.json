    },
    "devDependencies": {
        "@types/datejs": "^0.0.36",
        "@types/jest": "^29.5.0",
        "@types/node": "^22.0.0",
        "jest": "^29.5.0",
        "nodemon": "^3.1.7",
        "rimraf": "^6.0.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.0",
        "typescript": "^5.0.0"
    }
}
